<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.audio.pearljamlive"
       name="Pearl Jam Live"
       version="1.0.1"
       provider-name="Kinkin">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>A Pearl Jam fan addon!</summary>
        <description>Listen to Pearl Jam concert bootlegs as far back as 1990</description>
    </extension>

</addon>

<addon id="plugin.video.ultimateguitar"
       name="Ultimate Guitar"
       version="1.0.1"
       provider-name="Kinkin">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Learn to play guitar!</summary>
        <description>A complete guitar course from Justin Sandercoe, for beginners to advanced players</description>
    </extension>

</addon>

<addon id="plugin.video.whatthefurk"
       name="What the Furk"
       version="1.5.2a"
       provider-name="Batch,Kinkin,Mikey1234">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service"
             	library="service.py" start="startup">
	</extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>An addon which lets the user browse and stream videos from Furk.net.</summary>
        <description>Visit www.xbmchub.com for support.

What the Furk provides the user the ability do do IMDB powered media searches on Furk.net and gives the user the ability to stream the found media files.</description>
    </extension>

</addon>

<addon id="plugin.video.XMLbackup"
       name="XML Backup"
       version="1.0.2"
       provider-name="Kinkin">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>XML Backup</summary>
        <description>Backup all your addon and xbmc userdata xml settings in one easy step</description>
    </extension>

</addon>

<addon id="repository.Kinkin" name="Kinkins REPO" version="1.2" provider-name="Kinkin">
	<extension point="xbmc.addon.repository" name="Kinkin Addon Repository">
		<info compressed="false">http://kinkin-xbmc-repository.googlecode.com/svn/trunk/addons.xml</info>
		<checksum>http://kinkin-xbmc-repository.googlecode.com/svn/trunk/addons.xml.md5</checksum>
		<datadir zip="true">http://kinkin-xbmc-repository.googlecode.com/svn/trunk/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Kinkins Addons</summary>
		<description>Download and install addons suppported by Kinkin.......Thanks to Batch for a great addon......For official support visit xbmchub.com</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
